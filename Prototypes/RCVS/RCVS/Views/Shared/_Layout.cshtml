<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<title>@ViewBag.Title - Royal College of Veterinary Surgeons</title>
	<link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
	<meta name="viewport" content="width=device-width" />
	@Styles.Render("~/Content/css")
</head>
<body>
	<header>
	</header>

	<div id="body">
		@RenderSection("featured", required: false)
		<section class="content-wrapper main-content clear-fix">
			<div class="wrap three-columns">
				<div class="page">
					<div class="pagehead wrap three-columns">
						<div id="logocontainer">
							<a href="/home/" title="RCVS home" class="logo">
								<img src="../../Images/rcvs-logo.png" alt="RCVS logo">
							</a>

							<p class="tagline">Welcome to the Royal College of Veterinary Surgeons</p>



						</div>
						<div class="search">
							<a href="/search-tips/" id="search_tips"><span class="icon"></span><span class="label">Search tips</span></a>
							<form action="/search/">
								<fieldset>
									<legend>Site Search</legend>
									<div>
										<input id="sitesearch-keywords" type="text" name="keywords" value="What are you looking for?" class="input_text value_toggle" title="What are you looking for?">
									</div>
								</fieldset>
							</form>

							<div class="login">
								@Html.Partial("_LoginPartial")
							</div>


						</div>
					</div>
					<div class="pagemenu wrap three-columns">
						<ul id="nav">
							<li id="nav_home">
								<a href="/home/">Home</a>
							</li>
							<li id="nav_about-us">
								<a href="/about-us/">About</a>
							</li>
							<li id="nav_news-and-events">
								<a href="/news-and-events/">News & events</a>
							</li>
							<li id="nav_registration" class="active">
								<a href="/registration/" class="active">Registration</a>
							</li>
							<li id="nav_advice-and-guidance">
								<a href="/advice-and-guidance/">Advice & guidance</a>
							</li>
							<li id="nav_education">
								<a href="/education/">Education</a>
							</li>
							<li id="nav_complaints">
								<a href="/complaints/">Complaints</a>
							</li>
							<li id="nav_practice-standards-scheme">
								<a href="/practice-standards-scheme/">Practice Standards</a>
							</li>
							<li id="nav_publications">
								<a href="/publications/">Publications</a>
							</li>


						</ul>
					</div>
					<div class="pagebody">
						<div class="pagesubmenu">
							<ul class="nav">
								<li><span class="arrow"></span><a href="/registration/about-the-rcvs-register/"><span>About the RCVS Register</span></a></li>

								<li><span class="arrow"></span><a href="/registration/about-the-vn-register/"><span>About the VN Register</span></a></li>

								<li><span class="arrow"></span><a href="/registration/check-the-register-list/"><span>Check the Register/List</span></a></li>

								<li><span class="arrow"></span><a href="/registration/who-can-register/"><span>Who can register?</span></a></li>

								<li><span class="arrow"></span><a href="/registration/how-to-register/"><span>How to register</span></a></li>

								<li><span class="arrow"></span><a href="/registration/renewing-your-registration-listing/"><span>Renewing your registration/listing</span></a></li>

								<li><span class="arrow"></span><a href="/registration/amending-your-registration-listing/"><span>Amending your registration/listing</span></a></li>

								<li><span class="arrow"></span><a href="/registration/voluntary-removal-and-restoration/"><span>Voluntary removal and restoration</span></a></li>

								<li><span class="arrow"></span><a href="/registration/letters-of-good-standing/"><span>Letters of good standing</span></a></li>

								<li><span class="arrow"></span><a href="/registration/replacement-membership-certificates/"><span>Replacement Membership Certificates</span></a></li>

								<li><span class="arrow"></span><a href="/registration/register-of-veterinary-practice-premises/"><span>Register of Veterinary Practice Premises</span></a></li>

								<li><span class="arrow"></span><a href="/registration/statutory-membership-exam/"><span>Statutory Membership Exam</span></a>
									<ul>
										<li><a href="@Url.Action("Login", "Account")">Login</a></li>
										<li><a href="@Url.Action("Register", "Account")">Register</a></li>
										<li><a href="@Url.Action("Form1A2", "Home")">Initial declaration of intention to sit</a></li>

									</ul>
								</li>

								<li><span class="arrow"></span><a href="/registration/veterinary-nurse-pre-registration-examination/"><span>VN pre-registration examinations</span></a></li>

								<li><span class="arrow"></span><a href="/registration/contact-the-registration-department/"><span>Contact the Registration Department</span></a></li>

							</ul>


							<ul id="site-tools">
								<li id="tool-title">Web Tools</li>
								<li id="tool-print"><a href="/registration/?print=true"><span class="icon"></span><span class="text">Print this page</span></a></li>
								<li id="tool-sendtofriend"><a href="/registration/?action=show&amp;module=send-to-friend"><span class="icon"></span><span class="text">Send to a friend</span></a></li>
								<li id="tool-feedback"><a href="/feedback/?contact_type_ref=2"><span class="icon"></span><span class="text">Give us feedback</span></a></li>
								<li id="tool-contact"><a href="/about-us/contact-us/"><span class="icon"></span><span class="text">Contact us</span></a></li>
							</ul>
						</div>
						<div class="pagecontent">
							@RenderBody()
						</div>
					</div>
					<div class="pagetools">
						<div class="menu">
							<ul>
								<li><a href="/contact2/">Contact</a>
									&nbsp;|&nbsp;</li>

								<li><a href="/feedback1/">Feedback</a>
									&nbsp;|&nbsp;</li>

								<li><a href="/site-map/">Site map</a>
									&nbsp;|&nbsp;</li>

								<li><a href="/a-to-z/">A to Z</a>
									&nbsp;|&nbsp;</li>

								<li><a href="/accessibility/">Accessibility</a>
									&nbsp;|&nbsp;</li>

								<li><a href="/privacy-policy/">Privacy policy</a>
									&nbsp;|&nbsp;</li>

								<li><a href="/cookie-policy/">Cookie policy</a>
									&nbsp;|&nbsp;</li>

								<li><a href="/document-library/">Document library</a>
								</li>
							</ul>
						</div>


						<div class="copyright">&copy; Copyright <span id="js-site-name">RCVS</span> 2013</div>
					</div>
				</div>
			</div>
		</section>
	</div>
	<footer>
		<div class="content-wrapper">
			<div class="float-left">
				<p>&copy; @DateTime.Now.Year</p>
			</div>
		</div>
	</footer>

	@Scripts.Render("~/bundles/jquery")
	@RenderSection("scripts", required: false)
</body>
</html>
